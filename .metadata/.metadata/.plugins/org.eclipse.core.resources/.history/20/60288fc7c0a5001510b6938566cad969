package com.kiwilandtrains.core;

import java.util.ArrayList;
import java.util.LinkedHashSet;

public class Path {

	// list should maintain insertion order. this is essential
	LinkedHashSet<CityVertex> nodeList;
	
	public Path() {
		nodeList = new LinkedHashSet<String>();
	}
	
	public void addCityVertex(CityVertex cv) {
		nodeList.add(CityVertex);
	}
}
